{
  "name": "pocketpal-e2e",
  "version": "1.0.0",
  "description": "E2E tests for PocketPal using Appium + WebDriverIO",
  "scripts": {
    "test:android": "wdio run wdio.android.conf.ts",
    "test:ios": "wdio run wdio.ios.conf.ts",
    "test:android:local": "wdio run wdio.android.local.conf.ts",
    "test:ios:local": "wdio run wdio.ios.local.conf.ts",
    "test:aws": "node scripts/run-aws-device-farm.js",
    "typecheck": "tsc --noEmit"
  },
  "packageManager": "yarn@1.22.22",
  "dependencies": {
    "@wdio/appium-service": "^9.4.5",
    "@wdio/cli": "^9.4.5",
    "@wdio/local-runner": "^9.4.5",
    "@wdio/mocha-framework": "^9.4.4",
    "@wdio/spec-reporter": "^9.4.4",
    "appium": "^2.15.1",
    "appium-uiautomator2-driver": "^3.8.1",
    "appium-xcuitest-driver": "^7.32.5",
    "@aws-sdk/client-device-farm": "^3.693.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@wdio/globals": "^9.4.5",
    "dotenv": "^16.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
